{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 255,
      "default": "There is no title"
    },
    "description": {
      "type": "string",
      "required": true,
      "maxLength": 255,
      "default": "there is no description"
    },
    "content": {
      "type": "richtext",
      "required": true,
      "default": "<h1>There is no content</h1>"
    },
    "slug": {
      "type": "uid",
      "required": true,
      "maxLength": 255
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "posts"
    },
    "img": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    }
  }
}
